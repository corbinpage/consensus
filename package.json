{
  "name": "consensus",
  "version": "0.0.1",
  "description": "Simple honest product reviews.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "migrate": "node dist/db/migrate.js",
    "seed": "node dist/db/seed.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Corbin Page",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "pg": "^6.1.0",
    "resource-router-middleware": "^0.5.1",
    "sequelize": "^3.24.3",
    "tai-password-strength": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "http-server": "^0.9.0",
    "nodemon": "^1.9.2",
    "sqlite3": "^3.1.4"
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "keywords": [
    "product reviews",
    "node",
    "express"
  ]
}
